<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <gradient android:angle="270" android:endColor="#FF8A30" android:startColor="#FFA35B" />
            <corners android:radius="5dp" />
            <padding
                android:left="10dp" android:right="10dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#FF8D34"
                android:endColor="#F66C02"
                android:angle="270" />

            <corners android:radius="5dp" />
            <padding
                android:left="10dp" android:right="10dp" />
        </shape>
    </item>
</selector>
